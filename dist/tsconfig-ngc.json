{
	"compilerOptions": {
		"module": "es2015",
		"target": "es2016",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"declaration": true,
		"sourceMap": true,
		"moduleResolution": "node",
		"lib": [
			"es2015",
			"dom"
		],
		"allowSyntheticDefaultImports": true
	},
	"angularCompilerOptions": {
		"strictMetadataEmit": true,
		"skipTemplateCodegen": true
	}
}